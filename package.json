{
  "name": "jsonnet",
  "displayName": "Jsonnet",
  "description": "Language support for Jsonnet",
  "version": "0.0.2",
  "publisher": "heptio",
  "license": "SEE LICENSE IN 'LICENSE' file",
  "engines": {
    "vscode": "^1.10.0"
  },
  "categories": [
    "Languages"
  ],
  "homepage": "https://github.com/heptio/vscode-jsonnet/blob/master/README.md",
  "repository": {
    "type": "git",
