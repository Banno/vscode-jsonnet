// A launch configuration that launches the extension inside a new window
{
    "version": "0.1.0",
    "configurations": [
        {
            "name": "Attach",
            "type": "node",
            "request": "attach",
            "port": 6009,
            "sourceMaps": true,
            "stopOnEntry": true,
            "outFiles": [ "${workspaceRoot}/../out/server/**/*.js" ],
            "protocol": "legacy"
        }
    ]
}